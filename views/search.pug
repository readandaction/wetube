extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 Searching by #{searchingBy}
    .search__videos
        if videos.length === 0
            h5 No Video 
        each item in videos
            +videoBlock({
                title: item.title,
                description: item.description,
                videoFile:item.videoFile,
                id:item.id
            })